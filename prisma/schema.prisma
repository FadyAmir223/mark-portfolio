generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Type {
  COMMERCIAL
  RESEDENTIAL
}

model Project {
  id   String @id @default(cuid())
  type Type
  name String

  description_ar String
  description_en String

  video  String
  images String[]

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
